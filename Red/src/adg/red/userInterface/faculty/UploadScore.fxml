<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<VBox id="VBox" alignment="CENTER" spacing="5.0" AnchorPane.bottomAnchor="0"   AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns:fx="http://javafx.com/fxml" fx:controller="adg.red.controllers.faculty.UploadScoreController">
    <children>
        <HBox id="HBox" alignment="CENTER_LEFT" spacing="30.0">
            <children>
                <Label fx:id="lblHeading2" text="You are about to upload score for:" />
                <HBox id="HBox" alignment="CENTER" spacing="5.0">
                    <children>
                        <Label fx:id="lblDeptCoureNumber" text="CICS 520" />
                        <Label fx:id="lblSection" text="Secion" />
                        <Label fx:id="lblSecNumber" text="sectionNumber" />
                        <Label id="lblTerm" fx:id="lblSession" text="Winter" />
                        <Label fx:id="lblYear" text="2013" />
                    </children>
                </HBox>
                <Button fx:id="btnDownloadTemp" mnemonicParsing="false" onAction="#downloadTemplate" text="Download Template" />

            </children>
        </HBox>
        <HBox id="HBox" alignment="CENTER_LEFT" spacing="30.0">
            <children>
                <Label fx:id="lblFile" text="File:" />
                <Label fx:id="lblFilePath" text="FilePath" />
            </children>
        </HBox>
        <ListView fx:id="lsvResult" prefHeight="120"  AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" />
        <HBox id="HBox" alignment="CENTER" spacing="5.0">
            <children>
                <Button fx:id="btnBrowseFile" mnemonicParsing="false" onAction="#browseFile" text="Browse File" />
                <Button fx:id="btnUpload" mnemonicParsing="false" onAction="#uploadScore" text="Upload Score" />
            </children>
        </HBox>


    </children>
</VBox>


