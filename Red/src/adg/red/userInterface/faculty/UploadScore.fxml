<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane minHeight="464.0" minWidth="642.0" xmlns:fx="http://javafx.com/fxml" fx:controller="adg.red.controllers.faculty.UploadScoreController">
  <children>
    <TableView id="tabCourse" fx:id="tabSection" layoutX="13.0" layoutY="46.0" onMouseClicked="#selectSection" prefHeight="131.0" prefWidth="615.0">
      <columns>
        <TableColumn minWidth="100.0" prefWidth="100.0" text="Department Id" fx:id="colDept" />
        <TableColumn prefWidth="55.0" text="Sections" fx:id="colSection" />
        <TableColumn minWidth="5.0" prefWidth="50.0" text="Type" fx:id="colType" />
        <TableColumn prefWidth="75.0" text="Start Date" fx:id="colStart" />
        <TableColumn prefWidth="75.0" text="End Date" fx:id="colEnd" />
        <TableColumn minWidth="20.0" prefWidth="150.0" text="Teaching Assistant" fx:id="colTA" />
      </columns>
    </TableView>
    <Label fx:id="lblHeading" layoutX="14.0" layoutY="22.0" text="Please select section to upload score" />
    <VBox id="VBox" alignment="CENTER" layoutX="20.0" layoutY="188.0" spacing="5.0">
      <children>
        <HBox id="HBox" alignment="CENTER_LEFT" spacing="30.0">
          <children>
            <Label fx:id="lblHeading2" text="You are about to upload score for:" />
            <HBox id="HBox" alignment="CENTER" spacing="5.0">
              <children>
                <Label fx:id="lblDeptCoureNumber" text="CICS 520" />
                <Label fx:id="lblSection" text="Secion" />
                <Label fx:id="lblSecNumber" text="sectionNumber" />
                <Label id="lblTerm" fx:id="lblSession" text="Winter" />
                <Label fx:id="lblYear" text="2013" />
              </children>
            </HBox>
          </children>
        </HBox>
        <HBox id="HBox" alignment="CENTER_LEFT" spacing="30.0">
          <children>
            <Label fx:id="lblFile" text="File:" />
            <Label fx:id="lblFilePath" text="FilePath" />
          </children>
        </HBox>
      </children>
    </VBox>
    <HBox id="HBox" alignment="CENTER" layoutX="228.0" layoutY="417.0" spacing="5.0">
      <children>
        <Button fx:id="btnBrowseFile" mnemonicParsing="false" onAction="#browseFile" prefHeight="32.0" text="Browse File" />
        <Button fx:id="btnUpload" mnemonicParsing="false" onAction="#uploadScore" prefHeight="32.0" text="Upload Score" />
      </children>
    </HBox>
    <Label id="loginErrMsg" fx:id="lblResponse" alignment="CENTER" contentDisplay="CENTER" layoutX="228.0" layoutY="382.0" text="You have not selected a section" textFill="#cc0000">
      <font>
        <Font name="System Bold" size="12.0" />
      </font>
    </Label>
    <ListView fx:id="lsvResult" layoutX="143.0" layoutY="232.0" prefHeight="141.0" prefWidth="356.0" />
  </children>
  <stylesheets>
    <URL value="@../css/mainForm.css" />
  </stylesheets>
</AnchorPane>
