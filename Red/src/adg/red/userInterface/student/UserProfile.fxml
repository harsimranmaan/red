                                        <FXCollections fx:factory="observableArrayList">
                                            <String fx:value="BC" />
                                            <String fx:value="AB" />
                                        </FXCollections>
                                    </items>
                                </ChoiceBox>
                                <ChoiceBox fx:id="countryChoiceBox" layoutX="266.0" layoutY="161.0" prefWidth="142.0">
                                    <items>
                                        <FXCollections fx:factory="observableArrayList">
                                            <String fx:value="Canada" />
                                            <String fx:value="US" />
                                        </FXCollections>
                                    </items>
                                </ChoiceBox>
                                <Label fx:id="errLabel" layoutX="273.0" layoutY="297.0" text="" />
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="Change Password">
                    <content>
                        <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <Button id="pwdSaveBtn" fx:id="pwdSaveBtn"  font="$x1" layoutX="182.0" layoutY="239.0" mnemonicParsing="false" text="SAVE" />
