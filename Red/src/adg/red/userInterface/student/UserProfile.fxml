                                <PasswordField id="newPwdTxt" fx:id="newPwdTxt" layoutX="261.0" layoutY="101.0" prefWidth="200.0" />
                                <PasswordField id="pwdReTxt" fx:id="pwdReTxt" layoutX="261.0" layoutY="148.0" prefWidth="200.0" />
                            </children>
                        </AnchorPane>
                    </content>
