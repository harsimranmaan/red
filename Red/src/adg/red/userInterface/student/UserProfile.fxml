                            <children>
                                <Button fx:id="contactSaveBtn" layoutX="186.0" layoutY="330.0" mnemonicParsing="false" text="SAVE">
                                    <font>
                                        <Font name="System Bold" size="13.0" fx:id="x1" />
                                    </font>
                                </Button>
                                <Button fx:id="contactCancelBtn" font="$x1" layoutX="335.0" layoutY="331.0" mnemonicParsing="false" text="CANCEL" />
                                <Label id="addrLabel1" font="$x1" layoutX="124.0" layoutY="34.0" text="Address Line 1 * :" />
