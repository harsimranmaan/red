                                <TextField fx:id="addrTxt1" layoutX="266.0" layoutY="31.0" prefWidth="200.0" />
                                <TextField fx:id="addrTxt2" layoutX="266.0" layoutY="63.0" prefWidth="200.0" />
                                <TextField fx:id="cityTxt" layoutX="266.0" layoutY="96.0" prefWidth="200.0" />
